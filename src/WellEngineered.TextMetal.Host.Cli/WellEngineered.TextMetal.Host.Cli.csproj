	<ItemGroup>
		<PackageReference Include="System.Data.SqlClient" Version="4.4.0" />
	</ItemGroup>

	<PropertyGroup>
		<DefineConstants>$(DefineConstants);LEAK_CHECK</DefineConstants>
	</PropertyGroup>

</Project>
